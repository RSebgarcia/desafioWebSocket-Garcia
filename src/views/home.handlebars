<div>
    <h1>see products</h1>
    <ul>
        {{#each products}}
        <li>
            <h3>{{title}}</h3>
            <p>{{description}}</p>
            <p>Code: {{code}}</p>
            <p>Price: ${{price}}</p>
            <p>Status: {{status}}</p>
            <p>Stock: {{stock}}</p>
            <p>Category: {{category}}</p>
            <p>Thumbnails:</p>
            <ul>
                {{#each thumbnails}}
                <li><img src="{{this}}" alt="Thumbnail"></li>
                {{/each}}
            </ul>
        </li>
        {{/each}}
    </ul>
</div>
<script src="/socket.io/socket.io.js"> </script>
<script src="./js/home.js"></script>